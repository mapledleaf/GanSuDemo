.div_table {
	border: 1px solid black;
	height:100%;
	overflow:auto;
	overflow: hidden;
}

.div_title{
	clear:Both;
	border-bottom:1px solid black;
	background:skyblue;
	font-size :16px;
	line-height: 25px;
	height: 25px;
}

.div_td_column {
	padding-right: 5px;
	background: #efefff;
	margin: 1px;
	width: 14%;
	float: left;
	font-size:14;
	text-align: right;
	vertical-align: middle;
	line-height: 25px;
	height: 25px;
	overflow:hidden;
	white-space:nowrap;
	text-overflow:ellipsis;
}

.div_td_data {
	font-size:14;
	float: left;
	padding-left: 5px;
	background: #ffffff;
	margin: 1px;
	width: 18%;
	overflow:hidden;
	white-space:nowrap;
	text-overflow:ellipsis;
	line-height: 25px;
	height: 25px;
	text-align: left;
	vertical-align: middle;
}

.div_td_data input {
	margin-top: 2px;
}

.div_tr {
	width: 100%;
	clear: both;
	border: none;
	margin-top: 1px;
	margin-bottom: 1px;
}
.todolist{
	border:1px solid black;
	position:absolute;
	right:1%;
	bottom:6px;
}
.todolist .todolist-header{
	background-color:skyblue;
	border-bottom:1px solid black;
	height:20px;
	cursor:move;
}
.todolist .todolist-header span{
	width:80px;
	font-family:ו;
	line-height:20px;
	padding-left:4px;
	float:left;
}
.todolist .todolist-header img{
	float:right;
	cursor:pointer;
	line-height:20px;
}
.todolist .todolist-content{
	background-color:#E7EAE3;
	overflow:auto;
}
.todolist .todolist-content .row{
	padding-top:3px;
	height:18px;
	clear:both;
}
.todolist .todolist-content .col{
	float:left;
	text-align:center;
	font-family:ו;
}